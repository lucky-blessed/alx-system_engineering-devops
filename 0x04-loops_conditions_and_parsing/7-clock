# Displays the time for 12 hours and 59 minutes.
>>>>>>> 44b218b0be73172d9373d91e16f52962c98b2fad
#   Displays hours from 0 to 12
#   Displays minutes from 1 to 59

hour=0

while [ $hour -le 12 ]
do
  echo "Hour: $hour"
  minute=1
  while [ $minute -le 59 ]
  do
    echo "$minute"
    (( minute++ ))
  done
  (( hour++ ))
done
